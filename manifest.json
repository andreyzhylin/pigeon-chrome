{
  "manifest_version": 2,
  "name": "Pigeon",
  "description": "Tool to test javascript code in the context of specified pages.",
  "version": "1.1.5",
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage",
    "notifications"
  ],
  "options_page": "src/index.html",
  "icons": {
    "16": "src/assets/img/pigeon16.png",
    "48": "src/assets/img/pigeon48.png",
    "128": "src/assets/img/pigeon128.png"
  }
}